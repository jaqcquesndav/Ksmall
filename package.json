{
  "name": "ksmall",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "metro": "./metro.config.cjs",
  "scripts": {
    "start": "node start-hermes.js start",
    "android": "node start-hermes.js run:android",
    "android-lan": "node start-hermes.js start --android --lan",
    "ios": "node start-hermes.js run:ios",
    "web": "node start-hermes.js start --web",
    "clean": "node -e \"import { rmSync } from 'fs'; rmSync('node_modules', { recursive: true, force: true });\" && npm cache clean --force && npm install",
    "clean:win": "rmdir /s /q node_modules && npm cache clean --force && npm install",
    "start:offline": "cross-env REACT_NATIVE_OFFLINE_MODE=true node start-hermes.js start",
    "start:online": "cross-env REACT_NATIVE_OFFLINE_MODE=false node start-hermes.js start --clear",
    "check": "node -e \"console.log('App dependencies ok!')\" && expo diagnostics",
    "android:clear": "node start-hermes.js start --android --lan --clear",
    "clear-cache": "node start-hermes.js start --clear",
    "debug": "node start-hermes.js start --dev-client",
    "android-clear": "node start-hermes.js start --android --clear",
    "check-deps": "npx depcheck",
    "reset-metro": "npx react-native-clean-project --remove-ios-build --remove-android-build --reset-yarn-lock --keep-node-modules",
    "reset-hermesvm": "npx react-native-clean-project --reset-cache",
    "start:simple": "node start-simple.js start",
    "android:simple": "node start-simple.js run:android",
    "clear:simple": "node start-simple.js start --clear"
  },
  "dependencies": {
