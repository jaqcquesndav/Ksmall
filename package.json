{
  "name": "ksmall",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "metro": "./metro.config.cjs",
  "scripts": {
    "start": "node start.js start",
    "start:hermes": "node start.js --mode=hermes start",
    "start:standard": "node start.js --mode=standard start",
    "android": "node start.js --mode=hermes run:android",
    "android-lan": "node start.js --mode=hermes start --android --lan",
    "ios": "node start.js --mode=hermes run:ios",
    "web": "node start.js --mode=hermes start --web",
    "clean": "node -e \"import { rmSync } from 'fs'; rmSync('node_modules', { recursive: true, force: true });\" && npm cache clean --force && npm install",
    "clean:win": "rmdir /s /q node_modules && npm cache clean --force && npm install",
    "start:offline": "node start.js --mode=hermes --offline start",
    "start:online": "node start.js --mode=hermes start --clear",
    "check": "node -e \"console.log('App dependencies ok!')\" && expo diagnostics",
    "android:clear": "node start.js --mode=hermes start --android --lan --clear",
    "clear-cache": "node start.js --mode=hermes start --clear",
    "debug": "node start.js --mode=hermes start --dev-client",
    "android-clear": "node start.js --mode=hermes start --android --clear",
    "check-deps": "npx depcheck",
    "reset-metro": "npx react-native-clean-project --remove-ios-build --remove-android-build --reset-yarn-lock --keep-node-modules",
    "reset-hermesvm": "npx react-native-clean-project --reset-cache",
    "build:apk": "node build-apk.js"
  },
  "dependencies": {
    "@craftzdog/react-native-buffer": "^6.0.5",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.10.1",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.6",
    "@types/react": "~18.0.27",
    "axios": "^1.7.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "expo": "~53.0.5",
    "expo-asset": "~11.1.4",
    "expo-av": "~15.1.4",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "~17.1.5",
    "expo-document-picker": "~13.1.5",
    "expo-file-system": "~18.1.8",
    "expo-font": "^13.3.1",
    "expo-image-manipulator": "~13.1.5",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-local-authentication": "~16.0.4",
    "expo-localization": "~16.1.5",
    "expo-location": "~18.1.4",
    "expo-modules-core": "^2.3.12",
    "expo-network": "~7.1.5",
    "expo-notifications": "~0.31.1",
    "expo-print": "~14.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.8",
    "expo-sqlite": "~15.2.9",
    "expo-status-bar": "~2.2.3",
    "i18next": "^22.4.15",
    "lz-string": "^1.5.0",
    "node-libs-react-native": "^1.2.1",
    "qrcode": "^1.5.4",
    "react": "19.0.0",
    "react-hook-form": "^7.52.1",
    "react-i18next": "^12.2.2",
    "react-native": "0.79.2",
    "react-native-auth0": "^4.5.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-paper": "^5.13.1",
    "react-native-randombytes": "^3.6.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-vector-icons": "^10.1.0",
    "react-redux": "^9.1.2",
    "simple-markdown": "^0.7.3",
    "uuid": "^11.1.0",
    "victory": "^37.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@expo/config-plugins": "^10.0.2",
    "@types/date-fns": "^2.6.3",
    "@types/node": "^22.15.12",
    "@types/react": "~19.0.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-redux": "^7.1.34",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "react-native-clean-project": "^4.0.3",
    "react-test-renderer": "^19.1.0"
  },
  "private": true
}
